{
	"store.custom#landing": {
		"blocks": [
			"flex-layout.row#banner-principal-landing",
      "flex-layout.row#banners-new-landing",
      "flex-layout.row#shelf1-landing",
      "flex-layout.row#shelf2-landing",
      "flex-layout.row#banners-new-landing-2",
      "flex-layout.row#shelf3-landing"
		]
	},
  "flex-layout.row#banner-principal-landing": {
    "title": "Banner Principal",
    "children": ["list-context.image-list#banner-principal-landing"],
    "props": {
      "fullWidth": true,
      "blockClass": "carousel"
    }
  },

  "list-context.image-list#banner-principal-landing": {
    "title": "Imagens Banner Principal",
    "children": ["slider-layout#banners"],
    "props": {
      "images": [
        {
          "image": "https://td2154.vteximg.com.br/arquivos/fullbanner-td2154.png",
          "mobileImage": "https://td2154.vteximg.com.br/arquivos/fullbanner-td2154-mobile.png"
        },
        {
          "image": "https://td2154.vteximg.com.br/arquivos/fullbanner-td2154.png",
          "mobileImage": "https://td2154.vteximg.com.br/arquivos/fullbanner-td2154-mobile.png"
        },
        {
          "image": "https://td2154.vteximg.com.br/arquivos/fullbanner-td2154.png",
          "mobileImage": "https://td2154.vteximg.com.br/arquivos/fullbanner-td2154-mobile.png"
        },
        {
          "image": "https://td2154.vteximg.com.br/arquivos/fullbanner-td2154.png",
          "mobileImage": "https://td2154.vteximg.com.br/arquivos/fullbanner-td2154-mobile.png"
        }
      ]
    }
  },

  "flex-layout.row#banners-new-landing": {
    "title": "Imagens Banners Novo",
    "children": ["custom-banners"],
    "props": {
      "blockClass": ["banners", "banners-grid", "landing"]
    }
  },

  "flex-layout.row#slider-content-1": {
    "children": ["flex-layout.col#slider-content-1"],
    "props": {
      "blockClass": "four-banners"
    }
  },

  "flex-layout.col#slider-content-1": {
    "children": [
      "list-context.image-list#slide-1"
    ]
  },

  "list-context.image-list#slide-1": {
    "title": "Sliders Mobile 1",
    "children": ["slider-layout#grid-banners-mobile"],
    "props": {
      "blockClass": ["double-slide-list"],
      "images": [
        {
          "image": "/arquivos/banner-4.png",
          "mobileImage": "/arquivos/banner-4.png"
        },
        {
          "image": "/arquivos/banner-4.png",
          "mobileImage": "/arquivos/banner-4.png"
        },
        {
          "image": "/arquivos/banner-4.png",
          "mobileImage": "/arquivos/banner-4.png"
        },
        {
          "image": "/arquivos/banner-4.png",
          "mobileImage": "/arquivos/banner-4.png"
        }
      ]
    }
  },

  "flex-layout.row#slider-content-2": {
    "children": ["flex-layout.col#slider-content-2"],
    "props": {
      "blockClass": "four-banners"
    }
  },

  "flex-layout.col#slider-content-2": {
    "children": [
      "list-context.image-list#slide-2"
    ]
  },

  "list-context.image-list#slide-2": {
    "title": "Sliders Mobile 2",
    "children": ["slider-layout#grid-banners-mobile"],
    "props": {
      "blockClass": ["double-slide-list"],
      "images": [
        {
          "image": "/arquivos/banner-4.png",
          "mobileImage": "/arquivos/banner-4.png"
        },
        {
          "image": "/arquivos/banner-4.png",
          "mobileImage": "/arquivos/banner-4.png"
        },
        {
          "image": "/arquivos/banner-4.png",
          "mobileImage": "/arquivos/banner-4.png"
        },
        {
          "image": "/arquivos/banner-4.png",
          "mobileImage": "/arquivos/banner-4.png"
        }
      ]
    }
  },

  "slider-layout#grid-banners-mobile": {
    "title": "4 Banners Mobile",
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "autoplay": {
        "timeout": 3000,
        "stopOnHover": true
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": "carousel-mobile"
    }
  },

  "flex-layout.row#banners-new-landing-2": {
    "title": "Imagens Banners 2 Novo",
    "children": ["custom-banners"],
    "props": {
      "blockClass": "banners"
    }
  },

  "flex-layout.row#shelf1-landing": {
    "children": ["flex-layout.col#shelf-landing"],
    "props": { "blockClass": ["container-shelf-landing-1", "ignore-arrow"] },
    "title": "Vitrine de Produtos"
  },
  "flex-layout.col#shelf-landing": {
    "children": ["rich-text#shelf-landing-title", "list-context.product-list#shelf-landing"]
  },
  "flex-layout.row#shelf2-landing": {
    "children": ["flex-layout.col#shelf2-landing"],
    "props": { "blockClass": ["container-shelf-landing-1", "ignore-arrow"] },
    "title": "Vitrine de Produtos 2"
  },
  "flex-layout.col#shelf2-landing": {
    "children": ["rich-text#shelf2-landing-title", "list-context.product-list#shelf-landing"]
  },
  "flex-layout.row#shelf3-landing": {
    "children": ["flex-layout.col#shelf3-landing"],
    "props": { "blockClass": ["container-shelf-landing-1", "ignore-arrow"] },
    "title": "Vitrine de Produtos 3"
  },
  "flex-layout.col#shelf3-landing": {
    "children": ["rich-text#shelf3-landing-title", "list-context.product-list#shelf-landing"]
  },
  "flex-layout.row#shelf-landing": {
    "children": ["flex-layout.col#shelf-landing"],
    "props": { "blockClass": "container-shelf-landing-1" }
  },
  "rich-text#shelf-landing-title": {
    "props": {
      "text": "## Lan√ßamentos",
      "blockClass": "shelfTitle"
    }
  },
  "rich-text#shelf2-landing-title": {
    "props": {
      "text": "## Mais vendidos",
      "blockClass": "shelfTitle"
    }
  },
  "rich-text#shelf3-landing-title": {
    "props": {
      "text": "## Outlet",
      "blockClass": "shelfTitle"
    }
  },
  "list-context.product-list#shelf-landing": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#shelf-landing-products"],
    "props": {
      "orderBy": "OrderByTopSaleDESC",
      "installmentCriteria": "MAX_WITHOUT_INTEREST"
    }
  },
  "slider-layout#shelf-landing-products": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "fullWidth": false,
      "blockClass": "shelf",
      "showPaginationDots": false
    }
  }
}
